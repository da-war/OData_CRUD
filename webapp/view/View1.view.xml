>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <Table id="idProducts">
            <columns>
                <Column id="id-title">
                    <Text text="ID" />
                </Column>
                <Column id="">
                    <Text />
                </Column>
                <Column id="">
                    <Text id="" text="" />
                </Column>
                <Column id="">
                    <Text id="ds" text="" />
                </Column>
                <Column>
                    <Text />
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Text text="{ID}" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Page>
</mvc:View>
