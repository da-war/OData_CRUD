<mvc:View
    controllerName="odatacrud.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <Table id="idProducts" items="{/results}">
            <columns>
                <Column id="id-title">
                    <Text id="idTitle" text="ID" />
                </Column>
            </columns>
            <items>
                <ColumnListItem id="columnListItems1">
                    <cells>
                        <Text id="idData" text="{ID}" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Page>
</mvc:View>
